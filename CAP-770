#include <iostream>
using namespace std;
int main() 
{
    int ritu[5];
    ritu[0] = 10;
    ritu[1] = 20;
    ritu[2] = 30;
    ritu[3] = 40;
    ritu[4] = 50;
    cout << "Elements of the array:" <<endl;
    for (int i = 0; i < 5; ++i) {
        cout << "Element at index " << i << ": " << ritu[i] << endl;
    }
    return 0;
}
